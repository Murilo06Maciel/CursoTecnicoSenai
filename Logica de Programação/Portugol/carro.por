programa {
  //Carro


  funcao inicio()
  {
    real distancia, velocidademedia, tempo
     
    escreva ("Qual foi a distância percorrida?: ")
    leia (distancia)
    escreva ("Qual foi a velocidade média?: ")
    leia (velocidademedia)

    tempo = velocidademedia*distancia
    

    escreva ("O tempo total da viagem foi de: ", tempo)
    
  }
}
