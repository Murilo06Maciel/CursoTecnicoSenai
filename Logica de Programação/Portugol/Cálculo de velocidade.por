programa {
//Cálculo de velocidade

  funcao inicio() 
  {
    real distancia, tempo, velocidade
    escreva ("digite o valor da distancia: ")
    leia (distancia)
    escreva ("digite o valor do tempo: ")
    leia (tempo)

    velocidade = distancia*tempo

    escreva("\nA velocidade é igual a: ", velocidade)
    
  }
}
