programa
{
	funcao inicio() 
	{
		inteiro numero, soma = 0, contador = 0
		
		escreva("Digite o número até o qual deseja somar: ")
		leia(numero)

    limpa()
    
		faca
    {
      contador ++
			soma = soma + contador 
    }
    enquanto (contador < numero)
		escreva("A soma de 1 até ", numero, " é: ", soma, "\n")
	}
}

